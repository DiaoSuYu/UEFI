# Install this package as a python alternative.
# PYTHON_VERSION=3.9
# PYTHON_PRIORITY=39
/usr/sbin/alternatives --install /usr/bin/python python /usr/bin/python3.9 39
/usr/sbin/alternatives --install /usr/bin/python3 python3 /usr/bin/python3.9 39
/usr/sbin/alternatives --install /usr/bin/pydoc3 pydoc3 /usr/bin/pydoc3.9 39

rm -f /usr/share/icons/hicolor/icon-theme.cache
